{
    "name": "medaqueno/swoole-basic",
    "description": "Testing swoole and basic extensions and utilities",
    "authors": [
        {
            "name": "Carlos Jim√©nez - medaqueno",
            "email": "medaqueno@gmail.com",
            "role": "Creator / Lead Developer"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1.0",
        "guzzlehttp/guzzle": "^7.0",
        "league/container": "^4.2",
        "nesbot/carbon": "^2.57",
        "nikic/fast-route": "^1.3",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "ext-decimal": "^1.1",
        "openswoole/ide-helper": "~4.9",
        "php-decimal/php-decimal": "^1.1",
        "php-decimal/stubs": "^1.1",
        "php-ds/php-ds": "^1.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "ukko/phpredis-phpdoc": "5.0.1-beta8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Bootstrap\\": "bootstrap/",
            "Kernel\\": "kernel/"
        },
        "files": [
            "kernel/Helpers/functions.php"
        ]
    },
    "scripts": {
        "stan": [
            "vendor/bin/phpstan analyse . --ansi --no-interaction --memory-limit=-1"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
