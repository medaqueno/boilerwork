{
    "name": "medaqueno/boilerwork",
    "description": "PHP Swoole Boilerplate applying some basic patterns",
    "authors": [
        {
            "name": "Carlos Jim√©nez - medaqueno",
            "email": "medaqueno@gmail.com",
            "role": "Creator / Lead Developer"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1.0",
        "medaqueno/boilerwork-kernel": "^0.4.1",
        "php-ds/php-ds": "^1.4"
    },
    "require-dev": {
        "openswoole/ide-helper": "~4.9",
        "deminy/counit": "^0.2.0",
        "phpunit/phpunit": "^9.5",
        "kwn/php-rdkafka-stubs": "^2.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Bootstrap\\": "bootstrap/"
        },
        "files": [
            "bootstrap/functions.php"
        ]
    },
    "scripts": {},
    "minimum-stability": "dev",
    "prefer-stable": true
}
